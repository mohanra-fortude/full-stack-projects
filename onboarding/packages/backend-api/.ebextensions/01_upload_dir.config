commands: 
    01_create_uploads_folder: 
        command: "sudo mkdir -p /var/app/user-files/"
        command: "sudo mkdir -p /var/app/user-files/profile-images"
    02_create_documents_folder:
        command: "sudo mkdir -p /var/app/user-files/documents"
    03_set_folder_permission: 
        command: "sudo chmod 755 /var/app/user-files/profile-images"
    04_set_documents_folder:
        command: "sudo chmod 755 /var/app/user-files/documents"
